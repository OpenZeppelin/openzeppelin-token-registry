{
  "name": "zeppelin-vouching-app",
  "author": "Zeppelin Solutions & Delta Camp",
  "license": "MIT",
  "version": "1.0.0",
  "description": "Vouch for EVM packages using ZEP tokens.",
  "main": "index.js",
  "scripts": {
    "test": "eslint src && standard --verbose | snazzy && node scripts/test.js",
    "build": "$(npm bin)/apply-registry -o artifacts && node --max-old-space-size=4096 scripts/build.js",
    "start": "node --max-old-space-size=4096 scripts/start.js",
    "apply-registry": "$(npm bin)/apply-registry -o artifacts",
    "reset-local": "rm -rf networks/1234.json && rm -rf build/contracts"
  },
  "devDependencies": {
    "babel-plugin-inline-react-svg": "^1.0.1",
    "babel-plugin-root-import": "^6.1.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "jsdom": "^13.0.0",
    "react-hot-loader": "^4.3.11",
    "snazzy": "^8.0.0",
    "standard": "^12.0.1",
    "truffle-deploy-registry": "^0.5.1"
  },
  "dependencies": {
    "@ant-design/icons": "^1.1.15",
    "@ant-design/icons-react": "^1.1.2",
    "@babel/core": "7.1.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.6",
    "@fortawesome/free-brands-svg-icons": "^5.4.1",
    "@fortawesome/free-regular-svg-icons": "^5.4.1",
    "@fortawesome/free-solid-svg-icons": "^5.4.1",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "@sentry/browser": "^4.5.3",
    "@svgr/webpack": "2.4.1",
    "animated": "^0.2.2",
    "apollo-cache": "^1.1.25",
    "apollo-cache-inmemory": "^1.4.2",
    "apollo-client": "^2.4.12",
    "apollo-link": "^1.2.6",
    "apollo-link-ethereum": "^0.0.22",
    "apollo-link-ethereum-resolver-ethersjs": "^0.0.22",
    "apollo-link-state": "^0.4.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "9.0.0",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.4",
    "babel-plugin-named-asset-import": "^0.2.2",
    "babel-preset-react-app": "^5.0.4",
    "bfj": "^6.1.1",
    "bn.js": "4.11.8",
    "bulma": "^0.7.2",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "chalk": "2.4.1",
    "classnames": "^2.2.6",
    "copy-webpack-plugin": "^4.5.4",
    "css-loader": "1.0.0",
    "date-fns": "^2.0.0-alpha.27",
    "debug": "^4.1.0",
    "dotenv": "6.0.0",
    "dotenv-expand": "4.2.0",
    "eslint": "5.6.0",
    "eslint-config-react-app": "^3.0.4",
    "eslint-loader": "2.1.1",
    "eslint-plugin-flowtype": "2.50.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.11.1",
    "ethers": "^4.0.23",
    "file-loader": "2.0.0",
    "fs-extra": "7.0.0",
    "graphql": "^14.0.2",
    "graphql-anywhere": "^4.1.27",
    "graphql-tag": "^2.10.0",
    "html-webpack-plugin": "4.0.0-alpha.2",
    "identity-obj-proxy": "3.0.0",
    "immutability-helper": "^2.8.1",
    "jest": "23.6.0",
    "jest-pnp-resolver": "1.0.1",
    "jest-resolve": "23.6.0",
    "lodash": "^4.17.11",
    "mini-css-extract-plugin": "0.4.3",
    "mixpanel-browser": "^2.26.0",
    "node-sass": "^4.9.4",
    "object-assign": "^4.1.1",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "parse-github-url": "^1.0.2",
    "pnp-webpack-plugin": "1.1.0",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.0.6",
    "postcss-safe-parser": "4.0.1",
    "prop-types": "^15.6.2",
    "qs": "^6.6.0",
    "react": "^16.7.0",
    "react-apollo": "^2.3.3",
    "react-app-polyfill": "^0.1.3",
    "react-async-autocomplete": "^1.1.0",
    "react-content-loader": "^3.4.2",
    "react-copy-to-clipboard": "^5.0.1",
    "react-css-transition-replace": "^3.0.3",
    "react-custom-scroll": "^4.0.0",
    "react-dev-utils": "^6.0.5",
    "react-dom": "^16.7.0",
    "react-ga": "^2.5.7",
    "react-helmet": "^5.2.0",
    "react-router-dom": "^4.3.1",
    "react-router-named-routes": "^0.0.23",
    "react-timeout": "^1.1.2",
    "react-tooltip": "^3.9.0",
    "react-transition-group": "^2.5.3",
    "resolve": "1.8.1",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.0",
    "terser-webpack-plugin": "1.1.0",
    "url-loader": "1.1.1",
    "webpack": "^4.0.0",
    "webpack-dev-server": "3.1.9",
    "webpack-manifest-plugin": "2.0.4",
    "workbox-webpack-plugin": "3.6.2",
    "yn": "^3.0.0"
  },
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "describe",
      "context",
      "before",
      "beforeEach",
      "after",
      "afterEach",
      "it",
      "expect"
    ]
  },
  "eslintConfig": {
    "rules": {
      "vars-on-top": [
        "error"
      ],
      "no-use-before-define": [
        "error",
        {
          "functions": true,
          "classes": true
        }
      ]
    },
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/scripts/jest/setup.js",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.test.{js,jsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  }
}
